{
  "name": "battle-rap-fix-server",
  "description": "API server for Battle Rap Fix.",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon server.js --exec babel-node --presets es2015,stage-2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 0
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "morgan": "^1.8.2"
  },
  "repository": "git@github.com:terrytilley/battle-rap-fix-server.git",
  "author": "Terry Tilley <terry@terrytilley.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0"
  }
}
