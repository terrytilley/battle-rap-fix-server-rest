{
  "name": "battle-rap-fix-server",
  "description": "API server for Battle Rap Fix.",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon server.js --exec babel-node",
    "migrate:dev": "knex migrate:latest --env development",
    "migrate:test": "knex migrate:latest --env test",
    "seed:dev": "knex seed:run --env development",
    "seed:test": "knex seed:run --env test",
    "rollback:dev": "knex migrate:rollback --env development",
    "rollback:test": "knex migrate:rollback --env test"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 0
    }
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "knex": "^0.13.0",
    "morgan": "^1.8.2",
    "objection": "^0.8.4",
    "pg": "^6.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0"
  },
  "repository": "git@github.com:terrytilley/battle-rap-fix-server.git",
  "author": "Terry Tilley <terry@terrytilley.com>",
  "license": "MIT"
}
